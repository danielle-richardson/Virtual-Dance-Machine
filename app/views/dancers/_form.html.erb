
<%= render partial: 'layouts/errors', locals: {obj: @dancer} %>
<br>

<%= form_for(@dancer) do |f|%>
  <div>
  <%= f.label :username %>
  <%= f.text_field :username %> 
  </div>
 
  <div>
  <%= f.label :email %>
  <%= f.text_field :email %>
</div>

<div>
  <%= f.label :password %>
  <%= f.password_field :password %> 
  </div>
  <br>
 
  <%= f.submit "Sign Up", class: "btn-primary" %>
  </div>
<% end %>